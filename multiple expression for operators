#include <stdio.h>

int main() {
    int x, y, z;

    
    x = 10, y = 20, z = 30;

    // Using multiple expressions in a single line
    int result = (x + y, y + z);

    printf("x = %d, y = %d, z = %d\n", x, y, z);
    printf("result = %d\n", result); // This will print the value of the last expression: y + z

    return 0;
}
